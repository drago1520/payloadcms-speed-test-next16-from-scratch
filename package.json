{
  "name": "next-16-from-scratch",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run services && next dev",
    "build": "next build --turbo",
    "start": "next start",
    "gen": "bun types && bun importmap",
    "types": "bun tsx ./node_modules/payload/bin.js generate:types",
    "importmap": "dotenv -e .env -- bun payload generate:importmap",
    "services": "docker compose up -d",
    "lint": "eslint"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "^3.62.0",
    "@payloadcms/email-nodemailer": "^3.62.0",
    "@payloadcms/next": "^3.62.0",
    "@payloadcms/richtext-lexical": "^3.62.0",
    "@posthog/nextjs-config": "^1.3.9",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "graphql": "^16.12.0",
    "next": "16.0.2-canary.3",
    "payload": "^3.62.0",
    "posthog-js": "^1.284.0",
    "posthog-node": "^5.11.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "sharp": "^0.34.4",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.2-canary.3",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5"
  },
  "trustedDependencies": [
    "@posthog/cli",
    "@sentry/cli",
    "core-js",
    "esbuild",
    "sharp",
    "unrs-resolver"
  ]
}
